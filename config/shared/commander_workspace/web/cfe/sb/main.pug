block scripts
  script.
      
      Handlebars.registerHelper('toHex16', function(context) {
        return sprintf("0x%04x", context);
      }); 
      
      PrevSubFullTemplate = "" +
        "{{#each value.Entry}}" +
        "<tr>" +
        "  <th>{{toHex16 MsgID}}</td>" +
        "  <td>{{Priority}}</td>" +
        "  <td>{{Reliability}}</td>" +
        "  <td>{{PipeID}}</td>" +
        "</tr>" +
        "{{/each}}";
        
      var tplPrevSubArea = Handlebars.compile(PrevSubFullTemplate);
      var srcPrevSubArea   = $("#CFE_ES_PrevSubs_Table").html();
      
      //sage.Subscribe('CFE_SB_PrevSubMsg_Full', function(record) {
      //  var template = tplPrevSubArea(record);
      //  $('#CFE_ES_PrevSubs_Table').html(template);
      //});
      
      function SendMemoryPoolStats()
      {        
        session.sendCommand('/CFS/CFE_ES/TlmPoolStats', [
          {name:'Application', value:'CFE_SB'},
          {name:'PoolHandle', value:parseInt($('#CFE_SB_MemPoolHandle').text()).toString()}
        ]);
      }
      
      //sage.Subscribe("CFE_ES_PoolStats_Handle", function(record) {
      //  var timeStamp = CfeTimeToJsTime(record.seconds, record.subseconds)
      //  $("#MEM_LastUpdate").text($.format.date(timeStamp, "MM/dd/yyyy HH:mm:ss.") + timeStamp.getMilliseconds().toString().paddingLeft("000"));
      //});
      
      //sage.onAfterTlmUpdate(UpdatePoolHandleColors);
      
      function UpdatePoolHandleColors() {
        if($('#CFE_SB_MemPoolHandle').text() === $('#CFE_ES_PoolStats_Handle').text())
        {
            $('#CFE_SB_MemPoolHandle').css({'color':'green'});
            $('#CFE_ES_PoolStats_Handle').css({'color':'green'}); 
        }
        else
        {
            $('#CFE_SB_MemPoolHandle').css({'color':'red'});
            $('#CFE_ES_PoolStats_Handle').css({'color':'red'}); 
        }
      }
   
