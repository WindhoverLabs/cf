.row.os-theme-dark
  article.col-xs-12.col-sm-12
    header
      span.widget-icon
        i.fa.fa-table
      h2 Command and Data Handling
    div.widget-body.table-responsive
      table.table.table-condensed
            tbody
              tr
                th Cmd Count
                td
                  div(data-cdr={tlm:[{name:'/CFE/ES_HK/CmdCounter'}],indicator:'text'}) ---
              tr
                th Error Count
                td
                  div(data-cdr={tlm:[{name:'/CFE/ES_HK/Payload.ErrCounter'}],indicator:'text'}) ---
    footer.btn-group(role="group")      
      button.btn.btn-primary(data-cdr={cmd:{name:'/CFE/ES_Noop'},indicator:'cmd'}) No-Op    
      button.btn.btn-primary(data-cdr={cmd:{name:'/CFE/ES_Reset'},indicator:'cmd'}) Reset
  article.col-xs-12.col-sm-12
        header
          span.widget-icon
            i.fa.fa-area-chart
          h2 Pixel Flow Graph
        div.widget-graph
          div(data-cdr={tlm:[{name:'/CFE/ES_HK/CmdCounter'},{name:'/CFE/ES_HK/Payload.CFEMinorVersion'}],label:['PRESET','CFEMV'],indicator:'dataplot'})
