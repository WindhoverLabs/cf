            ul.dropdown-menu.pull-right
              li
                a.padding-10.padding-top-0.padding-bottom-0(href='javascript:void(0);')
                  i.fa.fa-cog
                  |  Setting
              li.divider
              li
                a.padding-10.padding-top-0.padding-bottom-0(href='#ajax/profile.html')
